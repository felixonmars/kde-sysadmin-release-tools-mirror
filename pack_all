cat modules.git | while read i branch; do
    case "$i" in 
    	# Place for special handling during git migration for instance
        *)
          echo "Packing $i"
          ./pack $i;
          echo ""
        ;;
    esac
done

for i in `cat modules`; do
    echo "Packing $i"
    ./pack $i;
    echo ""
done
