#!/bin/bash -x

###### TODO port to SVN
exit 1

for i in `cat modules`; do

(
case $i in
  arts)
    tag=ARTS_1_4_0_BETA_2
    ;;
  *)
    tag=KDE_3_4_0_BETA_2
    ;;
esac

cd clean
cvscheck -mucra $i | xargs rm -f
cvs update $i
if test "$i" = "kde-l10n"; then
  cp ../language_list $i/subdirs
fi
../removestuff $i
cvs rtag -d $tag $i 
cvs tag $tag $i
)
done
