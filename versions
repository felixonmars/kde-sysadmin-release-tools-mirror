#!/bin/bash
kde_version=4.10.5
kde_branch=KDE/4.10

export kde_version kde_branch

function set_urls {
case $1 in
  kde-l10n)
    HEADURL=branches/stable/l10n-kde4
    subname=kde-l10n
    ;;
  oxygen-icons)
    HEADURL=branches/$kde_branch/$1
    DESTURL=tags/KDE/$kde_version/$1
    subname=$1
    ;;
  *)
    HEADURL=branches/$kde_branch/$1
    subname=$1
    ;;
esac

}

