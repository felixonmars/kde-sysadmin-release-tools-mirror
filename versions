kde_version=4.11.1
kde_branch=KDE/4.11

export kde_version kde_branch

function set_urls {
case $1 in
  kde-l10n)
    HEADURL=branches/stable/l10n-kde4
    subname=kde-l10n
    ;;
  oxygen-icons)
    HEADURL=branches/$kde_branch/$1
    subname=$1
    ;;
  *)
    HEADURL=branches/$kde_branch/$1
    subname=$1
    ;;
esac

}

