function set_urls {
case $1 in
  kde-l10n)
    HEADURL=branches/stable/l10n-kde4
    DESTURL=tags/KDE/4.0.2/kde-l10n
    subname=kde-l10n
    ;;
  oxygen-icons)
    HEADURL=trunk/kdesupport/$1
    DESTURL=tags/KDE/4.0.2/$1
    subname=$1
    ;;
  *)
    HEADURL=branches/KDE/4.9/$1
    DESTURL=tags/KDE/4.0.2/$1
    subname=$1
    ;;
esac

}

